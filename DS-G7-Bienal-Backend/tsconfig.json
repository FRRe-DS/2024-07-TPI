{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@graph-ql": ["src/applications/graph-ql"],
      "@graph-ql/*": ["src/applications/graph-ql/*"],
      "@rest": ["src/applications/rest"],
      "@rest/*": ["src/applications/rest/*"],
      "@business": ["src/business"],
      "@business/*": ["src/business/*"],
      "@constants": ["src/config/constants"],
      "@constants/*": ["src/config/constants/*"],
      "@factories": ["src/config/factories"],
      "@factories/*": ["src/config/factories/*"],
      "@exceptions": ["src/config/cross/exceptions"],
      "@exceptions/*": ["src/config/cross/exceptions/*"],
      "@guards": ["src/config/cross/guards"],
      "@guards/*": ["src/config/cross/guards/*"],
      "@handdles": ["src/config/cross/handdles"],
      "@handdles/*": ["src/config/cross/handdles/*"],
      "@interceptors": ["src/config/cross/interceptors"],
      "@interceptors/*": ["src/config/cross/interceptors/*"],
      "@pipes": ["src/config/cross/pipes"],
      "@pipes/*": ["src/config/cross/pipes/*"],
      "@strategy": ["src/config/cross/strategy"],
      "@strategy/*": ["src/config/cross/strategy/*"],
      "@mongo": ["src/database/mongo"],
      "@mongo/*": ["src/database/mongo/*"],
      "@mongo/repositories": ["src/database/mongo/repositories"],
      "@mongo/repositories/*": ["src/database/mongo/repositories/*"],
      "@mongo/schemas": ["src/database/mongo/schemas"],
      "@mongo/schemas/*": ["src/database/mongo/schemas/*"],
      "@prisma": ["src/database/prisma"],
      "@prisma/*": ["src/database/prisma/*"],
      "@dto": ["src/dto"],
      "@dto/*": ["src/dto/*"],
      "@interfaces": ["src/interfaces"],
      "@interfaces/*": ["src/interfaces/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"],
      "@shared": ["src/shared"],
      "@shared/*": ["src/shared/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@entities": ["src/entities"],
      "@entities/*": ["src/entities/*"]
    }
  }
}
